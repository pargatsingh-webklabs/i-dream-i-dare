        <strong style="font-size: 1.3em;">My Joined Groups</strong>
        <hr>
        <% if @joined_groups.empty? == false %>
          <ul class="nav nav-pills nav-stacked">
            <li class="nav-header">
            </li>
            <% @joined_groups.each do |g| %>
              <!-- If active user, changes background color of pill. -->
              <% if @active_group_id != nil && @active_group_id == g.id %>
                <li class="btn-default" id="active-user-in-list">
                  <a href="/user/group-dashboard/<%= g.id %>">
                    <%= g.name %>
                  </a>
                </li>
              <% else %>
                <li class="btn-default">
                  <a href="/user/group-dashboard/<%= g.id %>">
                    <%= g.name %>
                  </a>
                </li>
              <% end %>         
                <li class="divider"></li>
              <% end %>
        <% end %>

        <!-- My Group Invitations -->
        <strong style="font-size: 1.3em;">Group Invites</strong>
