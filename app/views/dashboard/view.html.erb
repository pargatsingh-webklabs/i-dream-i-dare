<% if current_user.is_super_admin? && 2 == 2 %>

	<%= render(:partial => "super_admin_dash") %>

<% elsif current_user.is_an_admin? %>

  <%= render(:partial => "admin_dash") %> 

<% elsif current_user.is_a_coach? %>

  <%= render(:partial => "coach_dash", :locals => { :parameters => params } ) %>

<% end %>

<% if current_user.is_a_coach == false && current_user.is_an_admin == false && current_user.is_super_admin == false %>

  <%= render(:partial => "client_dash") %>

<% end %>