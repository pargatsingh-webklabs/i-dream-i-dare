	<div class="kt-grid__item kt-grid__item--fluid kt-grid kt-grid--hor">
		  
		  <div class="kt-content  kt-grid__item kt-grid__item--fluid" id="kt_content">
			 <div class="kt-portlet kt-portlet--mobile">
				<div class="kt-portlet__head">
				  
				   <div class="kt-portlet__head-label">
					  <h3 class="kt-portlet__head-title">
						Mentorships
					  </h3>
				   </div>
           <p id="notice"><%= notice %></p>
            <div class="kt-portlet__head-label">
           <%= link_to 'Create New', new_mentorship_path, class: "btn btn-success" %>
         </div>

				</div>
				
				<div class="kt-portlet__body table-responsive p-0 mt-0">
				   <table class="table table-striped- table-bordered table-hover table-checkable mt-0" id="kt_table_7">
					  <thead>
						  <tr>
							<th>
							  Id
							</th>
							<th>
							  Coach
							</th>
							<th>
							  Client
							</th>
							<th>
							  Actions
							</th>
						  </tr>
						</thead>
					  <tbody>
						 <% if @public_mentorship_data != nil %>
							<% @public_mentorship_data.each do |m| %>
							  <% mentorshipModel = @all_mentorships.find_by_id(m[0]) %>
							  <tr>
								<th>
								  <%= m[0] %>
								</th>
								<th>
								  <%= m[1].first_name %> <%= m[1].last_name %>
								</th>
								<th>
								  <%= m[2].first_name %> <%= m[2].last_name %>
								</th>
								<th>
								  <%= link_to 'Show', mentorshipModel %>
								  <%= link_to 'Edit', edit_mentorship_path(mentorshipModel) %>
								  <%= link_to 'Destroy', mentorshipModel, method: :delete, data: { confirm: 'Are you sure?' } %>
								</th>
							  </tr> 
							<% end %>
						  <% end %>
					  </tbody>
				   </table>
				</div>
			 </div>
		  </div>
	   </div>






	

